{
  "name": "flippy-testbench",
  "version": "1.0.0",
  "description": "Testbench",
  "main": "./dist/main.js",
  "scripts": {
    "dev:electron": "NODE_ENV=development webpack --config webpack.electron.config.js --mode development && electron .",
    "dev:react": "NODE_ENV=development webpack serve --config webpack.react.config.js --mode development",
    "build:electron": "NODE_ENV=production webpack --config webpack.electron.config.js --mode production",
    "build:react": "NODE_ENV=production webpack --config webpack.react.config.js --mode production",
    "build": "npm run build:electron && npm run build:react",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "lhl2617",
  "license": "MIT",
  "build": {
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "productName": "Flippy from TuneApp",
    "icon": "assets/icon/favicon.ico",
    "appId": "tuneapp.flippy",
    "directories": {
      "output": "dist"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.9.0",
    "@teamsupercell/typings-for-css-modules-loader": "^2.4.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.1",
    "electron": "^8.2.2",
    "electron-builder": "^22.7.0",
    "html-webpack-plugin": "^4.2.0",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^6.0.0",
    "webpack": "^4.42.1",
    "webpack-dev-server": "^3.11.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.9",
    "antd": "^4.9.1",
    "core-js": "^3.7.0",
    "opensheetmusicdisplay": "^0.8.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.13.7"
  }
}
