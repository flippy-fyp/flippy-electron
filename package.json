{
  "name": "flippy-testbench",
  "version": "1.0.0",
  "description": "Testbench",
  "main": "./dist/main.js",
  "scripts": {
    "dev:electron": "NODE_ENV=development webpack --config webpack.electron.config.js --mode development && electron .",
    "dev:react": "NODE_ENV=development webpack serve --config webpack.react.config.js --mode development",
    "build:electron": "NODE_ENV=production webpack --config webpack.electron.config.js --mode production",
    "build:react": "NODE_ENV=production webpack --config webpack.react.config.js --mode production",
    "build": "npm run build:electron && npm run build:react",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "test": "ts-mocha -u tdd -r jsdom-global/register --paths src/**/*.spec.ts"
  },
  "keywords": [],
  "author": "lhl2617",
  "license": "MIT",
  "build": {
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "productName": "Flippy Qualitative Testbench",
    "icon": "assets/icon/favicon.ico",
    "appId": "tuneapp.flippy",
    "directories": {
      "output": "dist"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@teamsupercell/typings-for-css-modules-loader": "^2.4.0",
    "@types/bluebird": "^3.5.33",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^8.2.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.10",
    "@types/shell-escape": "^0.2.0",
    "babel-loader": "^8.2.2",
    "canvas": "^2.6.1",
    "css-loader": "^5.0.1",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "html-webpack-plugin": "^4.5.0",
    "ignore-loader": "^0.1.2",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.2.1",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "ts-mocha": "^8.0.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "antd": "^4.9.4",
    "binary-search-bounds": "^2.0.4",
    "bluebird": "^3.7.2",
    "core-js": "^3.8.1",
    "opensheetmusicdisplay": "^0.8.7",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "regenerator-runtime": "^0.13.7",
    "typescript": "^4.1.3"
  }
}
